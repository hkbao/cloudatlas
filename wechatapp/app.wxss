/**app.wxss**/
@import "weui.wxss";
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.page__hd {
  padding: 20px;
}

.page__title {
  text-align:left;
  font-size:20px;
  font-weight:400;
}

.page__desc {
  margin-top:5px;
  color:#888888;
  text-align:left;
  font-size:14px;
}

.page {
  font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}

.weui-cell {
  border-top: 1rpx solid #d9d9d9;
}

.weui-cell::before {
  border-top: none;
}